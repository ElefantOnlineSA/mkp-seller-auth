{
  "memory": 256,
  "ttl": 10,
  "timeout": 10000,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "healthcheck": {
      "path": "/mkp-seller-auth/v0/healthcheck",
      "public": true
    },
    "configRegistration": {
      "path": "/mkp-seller-auth/v0/api/mkp-category-mapper/connector/configuration/:mapperId",
      "public": true
    },
    "mapperBrands": {
      "path": "/mkp-seller-auth/v0/api/suggestion/:marketplaceAccount/suggestions/mapping/seller/:sellerAccount/register/brand",
      "public": true
    },
    "mapperCategories": {
      "path": "/mkp-seller-auth/v0/api/suggestion/:marketplaceAccount/suggestions/mapping/seller/:sellerAccount/register/category",
      "public": true
    },
    "notificationInventory": {
      "path": "/mkp-seller-auth/v0/api/notificator/:sellerAccount/changenotification/:idSku/inventory",
      "public": true
    },
    "notificationPrice": {
      "path": "/mkp-seller-auth/v0/api/notificator/:sellerAccount/changenotification/:idSku/price",
      "public": true
    },
    "mkpCategoryInfo": {
      "path": "/mkp-seller-auth/v0/api/catalog/pvt/category/:categoryId",
      "public": true
    },
    "mkpBrandInfo": {
      "path": "/mkp-seller-auth/v0/api/catalog_system/pvt/brand/:brandId",
      "public": true
    },
    "sendSKUSuggestion": {
      "path": "/mkp-seller-auth/v0/:marketplaceAccount/suggestions/:sellerAccount/:idSku",
      "public": true
    }
  }
}
