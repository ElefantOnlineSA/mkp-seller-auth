{
  "memory": 256,
  "ttl": 10,
  "timeout": 10000,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "healthcheck": {
      "path": "/mkp-seller-auth/v0/healthcheck",
      "public": true
    },
    "configRegistration": {
      "path": "/mkp-seller-auth/v0/api/mkp-category-mapper/connector/configuration/:mapperId",
      "public": true
    },
    "mapperBrands": {
      "path": "/mkp-seller-auth/v0/api/suggestion/:marketplaceAccount/suggestions/mapping/seller/:sellerAccount/register/brand",
      "public": true
    },
    "mapperCategories": {
      "path": "/mkp-seller-auth/v0/api/suggestion/:marketplaceAccount/suggestions/mapping/seller/:sellerAccount/register/category",
      "public": true
    },
    "notificationInventory": {
      "path": "/mkp-seller-auth/v0/api/notificator/:sellerAccount/changenotification/:sellerSkuId/inventory",
      "public": true
    },
    "notificationPrice": {
      "path": "/mkp-seller-auth/v0/api/notificator/:sellerAccount/changenotification/:sellerSkuId/price",
      "public": true
    },
    "getCategory": {
      "path": "/mkp-seller-auth/v0/api/catalog/pvt/category/:categoryId",
      "public": true
    },
    "getBrand": {
      "path": "/mkp-seller-auth/v0/api/catalog_system/pvt/brand/:brandId",
      "public": true
    },
    "sendSKUSuggestion": {
      "path": "/mkp-seller-auth/v0/:marketplaceAccount/suggestions/:sellerAccount/:sellerSkuId",
      "public": true
    },
    "getSellerSKUBinding": {
      "path": "/mkp-seller-auth/v0/api/sku-binding/pvt/skuseller/:sellerAccount/:sellerSkuId",
      "public": true
    },
    "getSellerSKUBindingsInfo": {
      "path": "/mkp-seller-auth/v0/api/sku-binding/pvt/skuseller/admin",
      "public": true
    },
    "activateSellerSKUBinding": {
      "path": "/mkp-seller-auth/v0/api/sku-binding/pvt/skuseller/activate/:sellerAccount/:sellerSkuId",
      "public": true
    },
    "deactivateSellerSKUBinding": {
      "path": "/mkp-seller-auth/v0/api/sku-binding/pvt/skuseller/inactivate/:sellerAccount/:sellerSkuId",
      "public": true
    },
    "removeSellerSKUBinding": {
      "path": "/mkp-seller-auth/v0/api/sku-binding/pvt/skuseller/remove/:sellerAccount/:sellerSkuId",
      "public": true
    }
  }
}
